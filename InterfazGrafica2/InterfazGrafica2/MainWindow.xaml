<Window x:Class="InterfazGrafica2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InterfazGrafica2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">

    <StackPanel Name="Panel" ButtonBase.Click="Panel_Click">
        <Button Content="Dale!" Margin="10" Width="60" Height="30" HorizontalAlignment="Left" Click="Button_Click"></Button>
        <Button Content="Enviar" Margin="10" Width="60"  Height="30" HorizontalAlignment="Center" Click="Button_Click_1"></Button> 
        <Button Content="Otros" Margin="10" Width="50" Height="30" HorizontalAlignment="Right"></Button>
        
    </StackPanel>
</Window>
